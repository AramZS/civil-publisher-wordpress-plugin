    "ethereumjs-util": "^5.2.0",
    "immutable": "^3.8.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.3.3",
    "web3": "0.20.3"
